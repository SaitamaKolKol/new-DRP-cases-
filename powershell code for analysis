cmd.exe" /s /c "(echo On Error Resume Next & echo Set WSHShell = WScript.CreateObject("WScript.Shell"^) & echo NTPServer = WSHShell.RegRead("HKEY_LOCAL_MACHINE\SOFTWARE\Policies\Microsoft\W32time\Parameters\NtpServer"^) & echo If NTPServer ^<^> "" Then & echo NTPServer = Left(NTPServer, InStrRev(NTPServer, ","^)-1^) & echo If NTPServer ^<^> "time.nist.gov" and NTPServer ^<^> "tick.usno.navy.mil" and NTPServer ^<^> "ntp-s1.cise.ufl.edu" and NTPServer ^<^> "ntp.colby.edu" and NTPServer ^<^> "navobs1.oar.net" and NTPServer ^<^> "gnomon.cc.columbia.edu" and NTPServer ^<^> "tick.gatech.edu" and NTPServer ^<^> "ntp.okstate.edu" and NTPServer ^<^> "navobs1.wustl.edu" and NTPServer ^<^> "tick.uh.edu" and NTPServer ^<^> "tick.usnogps.navy.mil" and NTPServer ^<^> "montpelier.caltech.edu" and NTPServer ^<^> "bigben.cac.washington.edu" and NTPServer ^<^> "tick.ucla.edu" and NTPServer ^<^> "usno.pa-x.dec.com" and NTPServer ^<^> "ntp.alaska.edu" and NTPServer ^<^> "tick.mhpcc.hpc.mil" Then & echo WScript.echo "NTPServer option of NTP Client settings : " ^& NTPServer ^& " - does not synchronize with one or more Federally maintained NTP stratum-1 servers: NIST Internet Time Servers (http://tf.nist.gov/tf-cgi/servers.cgi); U.S. Naval Observatory Stratum-1 NTP Servers (http://tycho.usno.navy.mil/ntp.html)." & echo End If & echo Else & echo WScript.echo "NTP client settings are not configured." & echo End If) > "C:\Windows\Temp"\NTPClientSettings.vbs & %SystemRoot%\system32\cscript /nologo "C:\Windows\Temp"\NTPClientSettings.vbs & del "C:\Windows\Temp"\NTPClientSettings.vbs
